<div style="width: 500px;margin: 0px auto;">

	<!-- <a href="/routing/employeegrid" class="">Back</a> -->
<form action="/routing/employeeEdit" method="POST" >
	{{#entities}}
	<div class="form-group">
		<label>First Name:</label>
		<input type="hidden" name="userid" value="{{ main_id }}">
		<input type="text" name="firstname" value="{{ first_name }}" class="form-control">
	</div>
	<div class="form-group">
		<label>Last Name:</label>
		<input type="text" name="lastname" value="{{ last_name }}" class="form-control">
	</div>
		<div class="form-group">
		<label>Role:</label>
		<input type="text" name="lastname" placeholder="Consumer" class="form-control" disabled>
	</div>
<!-- 	<div class="form-group">
		<label>Role:</label>
		    <select name="rolval" id="rolVal" value="{{ role }}" class="form-control mb-2">
                <option value="">Select</option>
    			<option value="Manager">Manager</option>
    			<option value="Team Lead">Team Lead</option>
    			<option value="Developer">Developer</option>
    			<option value="Tester">Tester</option>
            </select>
	</div> -->
	<div class="form-group">
		<label>Employee Id:</label>
		<input type="number" name="employeeval" value="{{ emp_id }}" class="form-control">
	</div>
	<div class="form-group">
		<label>Username:</label>
		<input type="text" name="username" value="{{ user_name }}" class="form-control">
	</div>
	<div class="form-group">
		<label>Password:</label>
		<input type="password" name="password" value="{{ password }}"  class="form-control">

	</div>
	<div class="form-group">
		<label>Mobile Number:</label>
		<input type="number" name="mobnumber" id="txtPhoneNo" value="{{ mob_no }}"  onkeypress="ValidateNo();" class="form-control" pattern="[1-9]{1}[0-9]{9}">
		 <span style="float:left;" id="errorMsg" class="dsNone error"></span>
		 <span style="float:left;" id="successMsg" class="dsNone success">
	</div>
	<div class="form-group">
		<label>Email:</label>
		<input type="email" name="email" value="{{ email }}" onkeypress="isEmail();" class="form-control">
	</div>
	<div class="form-group">
		<input type="submit" class="btn btn-primary"  name="submit">
	</div>
	{{/entities}}
</form>


</div>
